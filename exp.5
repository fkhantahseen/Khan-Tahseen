 #include<stdio.h> 
 
 int iterative_fact(int num){
     int fact = 1;
     for (int i=num; i>=1; i--){
         fact = fact * i;
     }
     return fact;
 }
  int factorial(int n){
        if(n == 0 || n == 1)
        return 1;
        else
        return n * factorial(n-1); //Recursive case
        
    }
    int main(){
    int num = 6;
    printf("Factorial of %d is %d\n", num, factorial(num));
    return 0;
    }
