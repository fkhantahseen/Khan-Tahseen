#include<stdio.h>
int main(){
    int i,n,a[10],largest;
    
    printf("Enter How Many Numbers: ");
    scanf("%d",&n);
    
    printf("Enter the Numbers: \n");
    for(i = 0; i < n;i++){
        scanf("%d",&a[i]);
    }
    largest = a[0];
    for(i = 1; i < n;i++){
        if(a[i]>largest){
            largest = a[i];
        }
    }
    printf("Largest Number = %d\n",largest);
    return 0;
}
